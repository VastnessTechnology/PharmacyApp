<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aandroid="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fragment_vital_list_Time_relative_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:clickable="true">

    <include
        android:id="@+id/fragment_vital_list_Time_tootlbar"
        layout="@layout/comman_view" />

    <View
        android:id="@+id/fragment_vital_list_Time_tootlbar_view"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@+id/fragment_vital_list_Time_tootlbar"
        android:background="@color/seperator_color" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/fragment_vital_list_Time_Recyclerview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fragment_vital_list_Time_tootlbar_view"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:scrollbars="vertical"
        />



</RelativeLayout>