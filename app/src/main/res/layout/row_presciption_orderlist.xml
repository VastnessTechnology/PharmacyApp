<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white">

    <RelativeLayout
        android:id="@+id/row_pres_order_relative_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_3sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_3sdp"
        android:background="@drawable/rowshadow"
        android:padding="@dimen/_3sdp">

        <RelativeLayout
            android:id="@+id/row_pres_order_relative_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_5sdp"
            android:padding="@dimen/_1sdp">

            <LinearLayout
                android:id="@+id/row_pres_order_linear_Name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_2sdp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/row_pres_order_textview_name"
                    style="@style/text_normal_black_18"
                    android:layout_width="@dimen/_200sdp"
                    android:text=""
                    android:textSize="@dimen/_13sdp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/row_pres_order_textview_view_image"
                    style="@style/text_normal_black_18"
                    android:layout_width="match_parent"
                    android:layout_gravity="right"
                    android:layout_marginRight="@dimen/_5sdp"
                    android:gravity="left"
                    android:text="@string/view_image"
                    android:textColor="@color/parrot_green"
                    android:textSize="@dimen/_10sdp"
                    android:textStyle="bold" />
            </LinearLayout>




            <LinearLayout
                android:id="@+id/row_pres_order_linear_order_Date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/row_pres_order_linear_Name"
                android:layout_marginTop="@dimen/_7sdp"
                android:orientation="horizontal"
                android:weightSum="100">

                <TextView
                    android:id="@+id/row_pres_order_textview_Order_date"
                    style="@style/text_normal_black_18"
                    android:layout_width="wrap_content"
                    android:text="@string/order_Date"

                    android:textSize="@dimen/_11sdp" />

                <TextView
                    style="@style/text_normal_black_18"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:gravity="center"
                    android:text=":"
                    android:textSize="@dimen/_12sdp" />

                <TextView
                    android:id="@+id/row_pres_order_textview_order_date_value"
                    style="@style/text_normal_black_18"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:text=""
                    android:textSize="@dimen/_11sdp" />
            </LinearLayout>





            <LinearLayout
                android:id="@+id/row_pres_order_linear_order_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/row_pres_order_linear_order_Date"
                android:layout_marginTop="@dimen/_7sdp"
                android:orientation="horizontal"

                android:weightSum="100">

                <TextView
                    android:id="@+id/row_pres_order_textview_Order_status"
                    style="@style/text_normal_black_18"
                    android:layout_width="wrap_content"
                    android:text="@string/order_status"
                    android:textSize="@dimen/_10sdp" />

                <TextView
                    style="@style/text_normal_black_18"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:gravity="center"
                    android:text=":"
                    android:textSize="@dimen/_12sdp" />

                <TextView
                    android:id="@+id/row_pres_order_textview_status_value"
                    style="@style/text_normal_black_18"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:text=""
                    android:textSize="@dimen/_10sdp" />
            </LinearLayout>




        </RelativeLayout>

        <View
            android:id="@+id/row_order_view"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/row_pres_order_relative_top"
            android:layout_marginTop="@dimen/_3sdp"
            android:visibility="gone"
            android:background="@color/toolbar_title" />



    </RelativeLayout>
</RelativeLayout>
